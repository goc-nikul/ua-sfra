    <custom-type type-id="ConstructorIOFeedData">
        <display-name xml:lang="x-default">Constructor IO Feed Data</display-name>
        <description xml:lang="x-default">Unmodified data for products and categories that we send to Constructor</description>
        <staging-mode>no-staging</staging-mode>
        <storage-scope>site</storage-scope>
        <key-definition attribute-id="sfccAttribute">
            <type>string</type>
            <min-length>0</min-length>
        </key-definition>
        <attribute-definitions>
            <attribute-definition attribute-id="constructorDataType">
                <display-name xml:lang="x-default">Type of Data</display-name>
                <description xml:lang="x-default">The type of data, e.g. Facet or Metadata, on Constructor</description>
                <type>enum-of-string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <select-multiple-flag>true</select-multiple-flag>
                <value-definitions>
                    <value-definition>
                        <display xml:lang="x-default">Facet</display>
                        <value>facet</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">Metadata</display>
                        <value>metadata</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="constructorFeedType">
                <display-name xml:lang="x-default">Constructor Feed Type</display-name>
                <description xml:lang="x-default">The type of Constructor feed&#13;
* master(item) OR&#13;
* variation(variant)</description>
                <type>enum-of-string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <select-multiple-flag>true</select-multiple-flag>
                <value-definitions>
                    <value-definition>
                        <display xml:lang="x-default">Master (parent product or stand-alone) feed</display>
                        <value>master</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">Variation product feed</display>
                        <value>variation</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="constructorKey">
                <display-name xml:lang="x-default">Constructor Key</display-name>
                <description xml:lang="x-default">Name of the field in Constructor</description>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
