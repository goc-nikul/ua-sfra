<?xml version="1.0" encoding="UTF-8"?>
<payment-settings xmlns="http://www.demandware.com/xml/impex/paymentsettings/2009-09-15">
    <payment-method method-id="GIFT_CERTIFICATE">
        <name xml:lang="x-default">Gift Certificate</name>
        <enabled-flag>false</enabled-flag>
        <processor-id>BASIC_GIFT_CERTIFICATE</processor-id>
    </payment-method>

    <payment-method method-id="CREDIT_CARD">
        <name xml:lang="x-default">Credit Card</name>
        <enabled-flag>false</enabled-flag>
        <processor-id>BASIC_CREDIT</processor-id>
    </payment-method>

    <payment-method method-id="BANK_TRANSFER">
        <name xml:lang="x-default">Bank Transfer</name>
        <enabled-flag>false</enabled-flag>
    </payment-method>

    <payment-method method-id="BML">
        <name xml:lang="x-default">Bill Me Later</name>
        <enabled-flag>false</enabled-flag>
        <processor-id>CYBERSOURCE_BML</processor-id>
    </payment-method>

    <payment-method method-id="TOSS_PAYMENTS_CARD">
        <name xml:lang="x-default">Toss (Card)</name>
        <name xml:lang="ko">신용카드(+카카오페이)</name>
        <enabled-flag>true</enabled-flag>
        <processor-id>TOSS_PAYMENTS</processor-id>
        <countries>
            <country country-code="KR"/>
        </countries>
    </payment-method>

    <payment-method method-id="NAVERPAY">
        <name xml:lang="x-default">Naver Pay</name>
        <name xml:lang="ko">네이버페이</name>
        <name xml:lang="ko-KR">네이버페이</name>
        <description xml:lang="x-default">&lt;a class="naverpay-information"&gt;네이버페이 안내 &amp;gt;&lt;/a&gt;</description>
        <image>naver.jpg</image>
        <enabled-flag>true</enabled-flag>
        <processor-id>NAVERPAY</processor-id>
        <countries>
            <country country-code="KR"/>
        </countries>
    </payment-method>

</payment-settings>
