<?xml version="1.0" encoding="UTF-8"?>
<tax xmlns="http://www.demandware.com/xml/impex/tax/2007-02-14">
    <tax-classes>
        <tax-class class-id="accessories">
            <display-name>Accessories</display-name>
            <description/>
        </tax-class>

        <tax-class class-id="exempt">
            <display-name>Exempt</display-name>
            <description/>
        </tax-class>

        <tax-class class-id="standard" default="true">
            <display-name>Standard</display-name>
            <description/>
        </tax-class>

        <tax-class class-id="youth">
            <display-name>Youth</display-name>
            <description/>
        </tax-class>

        <tax-class class-id="youth-shoe">
            <display-name>Youth Shoe</display-name>
            <description>Discounted tax Rate for Poland youth shoe products</description>
        </tax-class>

    </tax-classes>
    <tax-jurisdictions>
        <tax-jurisdiction jurisdiction-id="HK">
            <display-name/>
            <address-mappings>
                <address-mapping key1="HK" key2="-"/>
            </address-mappings>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="ID">
            <display-name/>
            <address-mappings>
                <address-mapping key1="Indonesia" key2="-"/>
                <address-mapping key1="ID" key2="-"/>
            </address-mappings>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="MY">
            <display-name/>
            <address-mappings>
                <address-mapping key1="MY" key2="-"/>
                <address-mapping key1="Malaysia" key2="-"/>
            </address-mappings>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="PH">
            <display-name/>
            <address-mappings>
                <address-mapping key1="PH" key2="-"/>
                <address-mapping key1="Philippines" key2="-"/>
            </address-mappings>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="SG">
            <display-name/>
            <address-mappings>
                <address-mapping key1="SG" key2="-"/>
                <address-mapping key1="Singapore" key2="-"/>
            </address-mappings>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="TH">
            <display-name/>
            <address-mappings>
                <address-mapping key1="TH" key2="-"/>
                <address-mapping key1="Thailand" key2="-"/>
            </address-mappings>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="default" default="true">
            <display-name>Default</display-name>
        </tax-jurisdiction>

    </tax-jurisdictions>
    <tax-rates>
        <tax-rate jurisdiction-id="ID" class-id="youth">0.1</tax-rate>

        <tax-rate jurisdiction-id="HK" class-id="youth">0</tax-rate>

        <tax-rate jurisdiction-id="PH" class-id="youth">0.12</tax-rate>

        <tax-rate jurisdiction-id="TH" class-id="youth">0.07</tax-rate>

        <tax-rate jurisdiction-id="SG" class-id="youth">0.07</tax-rate>

        <tax-rate jurisdiction-id="MY" class-id="youth">0</tax-rate>

        <tax-rate jurisdiction-id="default" class-id="youth">0</tax-rate>

        <tax-rate jurisdiction-id="ID" class-id="exempt">0</tax-rate>

        <tax-rate jurisdiction-id="HK" class-id="exempt">0</tax-rate>

        <tax-rate jurisdiction-id="PH" class-id="exempt">0</tax-rate>

        <tax-rate jurisdiction-id="TH" class-id="exempt">0</tax-rate>

        <tax-rate jurisdiction-id="SG" class-id="exempt">0</tax-rate>

        <tax-rate jurisdiction-id="MY" class-id="exempt">0</tax-rate>

        <tax-rate jurisdiction-id="default" class-id="exempt">0</tax-rate>

        <tax-rate jurisdiction-id="ID" class-id="accessories">0.1</tax-rate>

        <tax-rate jurisdiction-id="HK" class-id="accessories">0</tax-rate>

        <tax-rate jurisdiction-id="PH" class-id="accessories">0.12</tax-rate>

        <tax-rate jurisdiction-id="TH" class-id="accessories">0.07</tax-rate>

        <tax-rate jurisdiction-id="SG" class-id="accessories">0.07</tax-rate>

        <tax-rate jurisdiction-id="MY" class-id="accessories">0</tax-rate>

        <tax-rate jurisdiction-id="default" class-id="accessories">0</tax-rate>

        <tax-rate jurisdiction-id="ID" class-id="youth-shoe">0.1</tax-rate>

        <tax-rate jurisdiction-id="HK" class-id="youth-shoe">0</tax-rate>

        <tax-rate jurisdiction-id="PH" class-id="youth-shoe">0.12</tax-rate>

        <tax-rate jurisdiction-id="TH" class-id="youth-shoe">0.07</tax-rate>

        <tax-rate jurisdiction-id="SG" class-id="youth-shoe">0.07</tax-rate>

        <tax-rate jurisdiction-id="MY" class-id="youth-shoe">0</tax-rate>

        <tax-rate jurisdiction-id="default" class-id="youth-shoe">0</tax-rate>

        <tax-rate jurisdiction-id="ID" class-id="standard">0.1</tax-rate>

        <tax-rate jurisdiction-id="HK" class-id="standard">0</tax-rate>

        <tax-rate jurisdiction-id="PH" class-id="standard">0.12</tax-rate>

        <tax-rate jurisdiction-id="TH" class-id="standard">0.07</tax-rate>

        <tax-rate jurisdiction-id="SG" class-id="standard">0.07</tax-rate>

        <tax-rate jurisdiction-id="MY" class-id="standard">0</tax-rate>

        <tax-rate jurisdiction-id="default" class-id="standard">0</tax-rate>

    </tax-rates>
</tax>
