<?xml version="1.0" encoding="UTF-8"?>
<url-rules xmlns="http://www.demandware.com/xml/impex/urlrules/2012-12-01">
    <general-settings>
        <lower-case-urls>true</lower-case-urls>
        <blank-substitute>-</blank-substitute>
        <character-replacements-enabled>true</character-replacements-enabled>
        <character-replacements-for-page-url-enabled>true</character-replacements-for-page-url-enabled>
    </general-settings>
    <locale-settings>
        <type>path</type>
        <locale-mappings>
            <locale-mapping xml:lang="en-PH">en-ph</locale-mapping>
            <locale-mapping xml:lang="ko-KR">ko-kr</locale-mapping>
            <locale-mapping xml:lang="en-ID">en-id</locale-mapping>
            <locale-mapping xml:lang="en-SG">en-sg</locale-mapping>
            <locale-mapping xml:lang="x-default">default</locale-mapping>
            <locale-mapping xml:lang="en-MY">en-my</locale-mapping>
            <locale-mapping xml:lang="zh">zh</locale-mapping>
            <locale-mapping xml:lang="in-ID">in-id</locale-mapping>
            <locale-mapping xml:lang="zh-HK">zh-hk</locale-mapping>
            <locale-mapping xml:lang="en">en</locale-mapping>
            <locale-mapping xml:lang="en-HK">en-hk</locale-mapping>
            <locale-mapping xml:lang="ko">ko</locale-mapping>
        </locale-mappings>
    </locale-settings>
    <category-rule>[ [ constant, c], / ,[ attribute, ID ] ]</category-rule>
    <category-page-url-override>true</category-page-url-override>
    <category-trailing-slash>true</category-trailing-slash>
    <category-offline-url-generation>true</category-offline-url-generation>
    <category-search-refinement-url-enabled>true</category-search-refinement-url-enabled>
    <category-search-refinements xml:lang="x-default">
        <url-search-refinements>
            <search-refinement-name>gender</search-refinement-name>
        </url-search-refinements>
        <url-search-refinements>
            <search-refinement-name>colorgroup</search-refinement-name>
        </url-search-refinements>
        <url-search-refinements>
            <search-refinement-name>fittype</search-refinement-name>
        </url-search-refinements>
        <url-search-refinements>
            <search-refinement-name>enduse</search-refinement-name>
        </url-search-refinements>
        <url-search-refinements>
            <search-refinement-name>silhouette</search-refinement-name>
        </url-search-refinements>
        <url-search-refinements>
            <search-refinement-name>division</search-refinement-name>
        </url-search-refinements>
    </category-search-refinements>
    <product-rule>[ [constant, p], / ,[ [category, [attribute, displayName] ], / , [attribute, name] ] ]</product-rule>
    <product-page-url-override>true</product-page-url-override>
    <product-id-separator>/</product-id-separator>
    <folder-rule>[ [ constant, f], / ,[ attribute, ID ] ]</folder-rule>
    <folder-page-url-override>true</folder-page-url-override>
    <folder-trailing-slash>true</folder-trailing-slash>
    <folder-offline-url-generation>true</folder-offline-url-generation>
    <folder-search-refinement-url-enabled>false</folder-search-refinement-url-enabled>
    <content-rule>[ constant, t ]</content-rule>
    <content-page-url-override>true</content-page-url-override>
    <content-id-separator>/</content-id-separator>
    <pipeline-aliases xml:lang="x-default">
        <pipeline-alias pipeline="Account-PasswordReset">passwordreset</pipeline-alias>
        <pipeline-alias pipeline="Content-Show">p</pipeline-alias>
        <pipeline-alias pipeline="Home-Show">home</pipeline-alias>
        <pipeline-alias pipeline="Console-Start">console</pipeline-alias>
        <pipeline-alias pipeline="SizeCharts-ShowSizeChart">size-guide</pipeline-alias>
        <pipeline-alias pipeline="Login-CreateAccountModal">register</pipeline-alias>
        <pipeline-alias pipeline="Order-TrackOrder">guest-login</pipeline-alias>
        <pipeline-alias pipeline="Order-Confirm">order</pipeline-alias>
        <pipeline-alias pipeline="Account-Show">account</pipeline-alias>
        <pipeline-alias pipeline="Order-GuestReturns">returnlabel</pipeline-alias>
        <pipeline-alias pipeline="Account-LoggedOutDueToInactivity">logged-out</pipeline-alias>
        <pipeline-alias pipeline="Account-SetNewPasswordConfirm">confirmednewpassword</pipeline-alias>
        <pipeline-alias pipeline="Cart-Show">cart</pipeline-alias>
        <pipeline-alias pipeline="Account-EditProfile">my-account</pipeline-alias>
        <pipeline-alias pipeline="Account-SetNewPassword">setpassword</pipeline-alias>
        <pipeline-alias pipeline="Checkout-Begin">checkout</pipeline-alias>
        <pipeline-alias pipeline="GiftCard-Show">gift-card-landing</pipeline-alias>
        <pipeline-alias pipeline="Page-CountriesList">change-location</pipeline-alias>
        <pipeline-alias pipeline="StoreLocator-Find">store-locator</pipeline-alias>
        <pipeline-alias pipeline="Address-List">addresses</pipeline-alias>
        <pipeline-alias pipeline="PaymentInstruments-List">payment-methods</pipeline-alias>
        <pipeline-alias pipeline="Home-ErrorNotFound">404</pipeline-alias>
        <pipeline-alias pipeline="Wishlist-Show">saved-items</pipeline-alias>
        <pipeline-alias pipeline="Order-History">my-order-history</pipeline-alias>
        <pipeline-alias pipeline="Login-Show">login</pipeline-alias>
        <pipeline-alias pipeline="Search-Show">search</pipeline-alias>
        <pipeline-alias pipeline="Stores-Find">stores</pipeline-alias>
    </pipeline-aliases>
    <pipeline-trailing-slash>false</pipeline-trailing-slash>
    <pipeline-perform-redirect>true</pipeline-perform-redirect>
