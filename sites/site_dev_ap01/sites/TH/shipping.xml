<?xml version="1.0" encoding="UTF-8"?>
<shipping xmlns="http://www.demandware.com/xml/impex/shipping/2007-03-31">
    <shipping-method method-id="TH-standard-shipping" default="true">
        <display-name xml:lang="x-default">Standard</display-name>
        <description xml:lang="x-default">Thailand Standard Shipping</description>
        <online-flag>true</online-flag>
        <tax-class-id>standard</tax-class-id>
        <price-table>
            <amount order-value="0">80</amount>
            <amount order-value="2200">0</amount>
        </price-table>
        <custom-attributes>
            <custom-attribute attribute-id="isEmployeeShipping">false</custom-attribute>
            <custom-attribute attribute-id="isExpressShipping">false</custom-attribute>
            <custom-attribute attribute-id="isScheduledDelivery">false</custom-attribute>
        </custom-attributes>
    </shipping-method>

    <shipping-method method-id="COD-001-THB" default="false">
        <display-name xml:lang="x-default">THB COD</display-name>
        <description xml:lang="x-default">Thailand COD</description>
        <online-flag>true</online-flag>
        <price-table>
            <amount order-value="0">80</amount>
            <amount order-value="2200">0</amount>
        </price-table>
        <custom-attributes>
            <custom-attribute attribute-id="isEmployeeShipping">false</custom-attribute>
            <custom-attribute attribute-id="isExpressShipping">false</custom-attribute>
            <custom-attribute attribute-id="isScheduledDelivery">false</custom-attribute>
        </custom-attributes>
    </shipping-method>

</shipping>
