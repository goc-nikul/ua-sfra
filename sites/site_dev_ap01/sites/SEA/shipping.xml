    <shipping-method method-id="HK-standard-shipping" default="true">
        <display-name xml:lang="x-default">Standard</display-name>
        <display-name xml:lang="zh-HK">香港標準運費</display-name>
        <description xml:lang="x-default">Hong Kong standard shipping</description>
        <description xml:lang="zh-HK">香港標準運費</description>
        <online-flag>true</online-flag>
        <tax-class-id>standard</tax-class-id>
        <price-table>
            <amount order-value="0">30</amount>
