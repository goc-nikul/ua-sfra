<?xml version="1.0" encoding="UTF-8"?>
<shipping xmlns="http://www.demandware.com/xml/impex/shipping/2007-03-31">
    <shipping-method method-id="COD-001-PHP" default="false">
        <display-name xml:lang="x-default">PHP COD</display-name>
        <description xml:lang="x-default">Philippines COD</description>
        <online-flag>true</online-flag>
        <price-table>
            <amount order-value="0">180</amount>
            <amount order-value="2700">0</amount>
        </price-table>
        <custom-attributes>
            <custom-attribute attribute-id="isEmployeeShipping">false</custom-attribute>
            <custom-attribute attribute-id="isExpressShipping">false</custom-attribute>
            <custom-attribute attribute-id="isScheduledDelivery">false</custom-attribute>
        </custom-attributes>
        <currency>PHP</currency>
    </shipping-method>

