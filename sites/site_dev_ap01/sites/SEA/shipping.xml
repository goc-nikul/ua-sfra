    <shipping-method method-id="SG-standard-shipping-1" default="false">
        <display-name xml:lang="x-default">Standard</display-name>
        <description xml:lang="x-default">Singapore Standard Shipping-Member</description>
        <online-flag>true</online-flag>
        <tax-class-id>standard</tax-class-id>
        <price-table>
            <amount order-value="0">0</amount>
        </price-table>
        <excluded-addresses>
            <included-addresses>
                <condition-group match-mode="all">
                    <condition>
                        <attribute-path>shipment.shippingAddress.countryCode</attribute-path>
                        <operator>is-not-equal</operator>
                        <string>SG</string>
                    </condition>
                </condition-group>
            </included-addresses>
        </excluded-addresses>
        <custom-attributes>
            <custom-attribute attribute-id="isEmployeeShipping">false</custom-attribute>
            <custom-attribute attribute-id="isExpressShipping">false</custom-attribute>
            <custom-attribute attribute-id="isScheduledDelivery">false</custom-attribute>
        </custom-attributes>
        <currency>SGD</currency>
        <customer-groups>
            <customer-group>Registered</customer-group>
        </customer-groups>
    </shipping-method>

    <shipping-method method-id="PH-standard-shipping" default="true">
