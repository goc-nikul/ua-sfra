        <name xml:lang="x-default">Gift Card</name>
        <enabled-flag>true</enabled-flag>
        <processor-id>FIRST_DATA</processor-id>
    </payment-method>

    <payment-method method-id="VIP_POINTS">
        <name xml:lang="x-default">VIP Points</name>
        <enabled-flag>true</enabled-flag>
        <processor-id>VIP_POINTS</processor-id>
