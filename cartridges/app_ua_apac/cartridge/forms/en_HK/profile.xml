<?xml version="1.0"?>
<form xmlns="http://www.demandware.com/xml/form/2008-04-19" secure="false">
	<group formid="customer">
		<field 
			formid="firstname" 
			label="label.input.firstname.profile" 
			mandatory="true" 
			max-length="50"
			range-error="error.message.50orless" 
			type="string" />

		<field 
			formid="lastname" 
			label="label.input.lastname.profile" 
			mandatory="true" 
			max-length="50" 
			range-error="error.message.50orless" 
			type="string" />

		<field
			formid="email" 
			label="label.input.email.profile" 
			mandatory="true" 
			max-length="50" 
			missing-error="error.message.required"
			parse-error="error.message.parse.email.profile.form"
			range-error="error.message.50orless"
			regexp="^[\w.%+-]+@[\w.-]+\.[\w]{2,6}$" 
			value-error="ValueErrorText"
			type="string"/> 
			<!-- TODO value-error not comming back from the platform -->

        <field
            formid="newemail" 
            label="label.input.newemail" 
            mandatory="true" 
            max-length="50" 
            missing-error="error.message.required"
            parse-error="error.message.parse.email.profile.form"
            range-error="error.message.50orless"
            regexp="^[\w.%+-]+@[\w.-]+\.[\w]{2,6}$" 
            value-error="ValueErrorText"
            type="string"/>

        <field
            formid="newemailconfirm" 
            label="label.input.newemailconfirm" 
            mandatory="true" 
            max-length="50" 
            missing-error="error.message.required"
            parse-error="error.message.parse.email.profile.form"
            range-error="error.message.50orless"
            regexp="^[\w.%+-]+@[\w.-]+\.[\w]{2,6}$" 
            value-error="ValueErrorText"
            type="string"/>

