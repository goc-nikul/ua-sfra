<?xml version="1.0" encoding="UTF-8"?>
<custom-objects xmlns="http://www.demandware.com/xml/impex/customobject/2006-10-31">
    <custom-object type-id="RecipeCategories" object-id="on-model-bottom">
        <object-attribute attribute-id="DESCRIPTION">Used to capture photography for bottom-related products.</object-attribute>
        <object-attribute attribute-id="SEQUENCE">100</object-attribute>
    </custom-object>

    <custom-object type-id="RecipeCategories" object-id="on-model-bottom-women">
        <object-attribute attribute-id="DESCRIPTION">Used to capture photography for bottom-related products specifically geared towards women.</object-attribute>
        <object-attribute attribute-id="SEQUENCE">200</object-attribute>
    </custom-object>

    <custom-object type-id="RecipeCategories" object-id="on-model-mid">
        <object-attribute attribute-id="DESCRIPTION">Used to capture photography for mid-shot products (ex. shorts, underwear bottoms, etc).</object-attribute>
        <object-attribute attribute-id="SEQUENCE">400</object-attribute>
    </custom-object>

    <custom-object type-id="RecipeCategories" object-id="on-model-top">
        <object-attribute attribute-id="DESCRIPTION">Used to capture photography for tops (t-shirts, hoodies, polos, and other upper-torso related products).</object-attribute>
        <object-attribute attribute-id="SEQUENCE">500</object-attribute>
    </custom-object>


    <custom-object type-id="RecipeCategories" object-id="standard-0pad">
        <object-attribute attribute-id="DESCRIPTION">Used to capture photography with no padding applied to the image.</object-attribute>
        <object-attribute attribute-id="SEQUENCE">900</object-attribute>
    </custom-object>

    <custom-object type-id="RecipeCategories" object-id="standard-10pad">
        <object-attribute attribute-id="DESCRIPTION">Used to capture photography with 10% padding applied to the image.</object-attribute>
        <object-attribute attribute-id="SEQUENCE">1000</object-attribute>
    </custom-object>

    <custom-object type-id="RecipeCategories" object-id="standard-20pad">
        <object-attribute attribute-id="DESCRIPTION">Used to capture photography with 20% padding applied to the image.</object-attribute>
        <object-attribute attribute-id="SEQUENCE">1100</object-attribute>
    </custom-object>

    <custom-object type-id="RecipeCategories" object-id="standard-30pad">
        <object-attribute attribute-id="DESCRIPTION">Used to capture photography with 30% padding applied to the image.</object-attribute>
        <object-attribute attribute-id="SEQUENCE">1200</object-attribute>
    </custom-object>

</custom-objects>
