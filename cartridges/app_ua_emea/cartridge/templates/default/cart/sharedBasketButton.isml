                        </div>
                    </div>
                    <div class="g-flex">
                        <div class="g-flex_col">
                            <h2>${Resource.msg('label.share.qrcode','sharedBasket',null)}</h2>
                            <button class="g-button_base g-button_primary--black js-show-qrbtn" type="button">${Resource.msg('label.generate.qrcode','sharedBasket',null)}</button>
                            <div class="tab-pane active" id="js-qrcode-placeholder" role="tabpanel" aria-labelledby="qr-tab" data-logo="${URLUtils.staticURL('/images/ualogo_black.png')}"></div>
                        </div>
                    </div>
                </div>
                <h3 class="description">${Resource.msg('msg.cart.modal','sharedBasket',null)}</h3>
            </div>
        </div>
    </div>
</div>