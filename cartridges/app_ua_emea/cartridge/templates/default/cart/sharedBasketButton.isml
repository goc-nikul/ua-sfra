<div class="share-basket-button">
    <svg data-action-url="${URLUtils.https('QRcode-SharedBaskets')}" xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="currentColor" class="bi bi-share-fill js-qrbtn" viewBox="0 0 16 16">
        <path d="M11 2.5a2.5 2.5 0 1 1 .603 1.628l-6.718 3.12a2.499 2.499 0 0 1 0 1.504l6.718 3.12a2.5 2.5 0 1 1-.488.876l-6.718-3.12a2.5 2.5 0 1 1 0-3.256l6.718-3.12A2.5 2.5 0 0 1 11 2.5z"/>
    </svg>
</div>
<div  class="modal g-modal g-modal-loginform" id="qrModal" role="dialog" aria-modal="true">
    <div class="modal-dialog g-modal-dialog">
        <div class="modal-content g-modal-content share-modal">
            <div class="g-modal-header g-login-modal-header">
                <h2 class="g-modal-title g-password-modal-title request-password-title">${Resource.msg('label.share.basket','sharedBasket',null)}</h2>
                <div class="g-password-modal-close">
                    <button type="button" class="g-password-modal-close_button close"  id="closeModalBtn" data-dismiss="modal" aria-label="Close"></button>
                </div>
            </div>
            <div class="g-modal-body qrcode-m-body">
                <div class="qrcode-body-content">
                    <div class="g-flex">
                        <div class="g-flex_col">
                            <div class="g-flex_row">
                                <h2 class="copy-link">${Resource.msg('label.share.link','sharedBasket',null)}</h2>
                            </div>
                            <div class="g-flex_row g-flex_copylink">
                                <input class="display-link js-display-link" type="text" name="URLInput" readonly/>
                                <div class="icon-group">
                                    <div class="icon-wrapper">
                                        <div class="icon copy-btn" id="copyBtn">
                                            <svg class="clippy" width="22" height="22" viewBox="0 0 16 16" fill="none" stroke="currentColor" stroke-width="1.5"
                                                stroke-linecap="round" stroke-linejoin="round">
                                                <path d="M5.75 4.75H10.25V1.75H5.75V4.75Z" />
                                                <path
                                                    d="M3.25 2.88379C2.9511 3.05669 2.75 3.37987 2.75 3.75001V13.25C2.75 13.8023 3.19772 14.25 3.75 14.25H12.25C12.8023 14.25 13.25 13.8023 13.25 13.25V3.75001C13.25 3.37987 13.0489 3.05669 12.75 2.88379" />
                                            </svg>
                                            <svg class="check" width="22" height="22" viewBox="0 0 16 16" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"><path d="M13.25 4.75L6 12L2.75 8.75"></path></svg>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="g-flex">
                        <div class="g-flex_col">
                            <h2>
                                ${Resource.msg('label.share.whatsapp','sharedBasket',null)}
