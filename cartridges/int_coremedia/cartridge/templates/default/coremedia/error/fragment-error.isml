<iscontent type="text/html" charset="UTF-8" compact="true"/>
<div style="padding: 10px; margin: 2px; background-color: #F2DEDE; border-radius: 4px;display: inline-block; border: 1px solid #cc0000; color: #cc0000; text-transform: none;">
    <isif condition="${pdict.error > 0}">
        <a style="color: #cc0000;" href="${pdict.url}" target="_blank">CoreMedia fragment request returned with: <b>${pdict.error} ${pdict.message}</b></a>
    <iselse/>
        <b>${pdict.message}</b>
    </isif>
</div>
